<html>
    <body>
        <%-include('Nav')-%>
        <center><h2>Withdraw Amount</h2></center>
        <hr>
        <div className="row">
            <center>
            <div className="col-md-4"></div>
         <div className="col-md-5">
          
         <form>
           <div class="form-row"></div>
             <div class="form-group col-md-6">
               <label for="inputAccount">Enter Account Number</label>
               <input type="Account" class="form-control" id="inputAccount" placeholder="14 Digit Number" name="ac"></input>
             </div>
             <div class="form-row"></div>
             <div class="form-group col-md-6">
               <label for="inputPin">Enter Pin Number</label>
               <input type="Pin" class="form-control" id="inputPin" placeholder="4 Digit Number" name="p"></input>
             </div>
             <div class="form-row"></div>
             <div class="form-group col-md-6">
               <label for="inputWithdraw">Enter Amount to Withdraw</label>
               <input type="Withdraw" class="form-control" id="inputWithdraw" placeholder="Enter Amount" name="w"></input>
               <br></br>
               <input type="submit" name="submit" class="btn btn-success" value="Withdraw"></input> 
               <br></br>
             
             </div>
            </form>
            </div>
        </center>
        <br><hr>
        <h2><%=msg%></h2>
                 </div>
    </body>
</html>